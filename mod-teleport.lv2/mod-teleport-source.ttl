@prefix lv2:  <http://lv2plug.in/ns/lv2core#>.
@prefix doap: <http://usefulinc.com/ns/doap#>.
@prefix epp: <http://lv2plug.in/ns/ext/port-props#>.
@prefix foaf: <http://xmlns.com/foaf/0.1/>.
@prefix mod: <http://moddevices.com/ns/mod#>.
@prefix opts: <http://lv2plug.in/ns/ext/options#>.
@prefix rdf:  <http://www.w3.org/1999/02/22-rdf-syntax-ns#>.
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#>.
@prefix pset: <http://lv2plug.in/ns/ext/presets#>.
@prefix units: <http://lv2plug.in/ns/extensions/units#>.

<https://mod.audio/plugins/teleport#source>
a lv2:Plugin, lv2:UtilityPlugin, doap:Project;

mod:brand "MOD";
mod:label "Teleport Source";

doap:name "Teleport Source";

doap:maintainer [
    foaf:name "MOD Audio";
    foaf:homepage <https://mod.audio>;
    foaf:mbox <mailto:contact@mod.audio>;
];

doap:license <http://spdx.org/licenses/ISC.html>;

rdfs:comment """
...
""";

lv2:port
[
    a lv2:AudioPort, lv2:OutputPort;
    lv2:index 0;
    lv2:symbol "source_left";
    lv2:name "Source Left";
],
[
    a lv2:AudioPort, lv2:OutputPort;
    lv2:index 1;
    lv2:symbol "source_right";
    lv2:name "Source Right";
].
